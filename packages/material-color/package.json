{
    "name": "@ui-schema/material-color",
    "version": "0.5.0-alpha.1",
    "description": "React color pickers as MUI widgets for UI-Schema - using react-color.",
    "author": "Michael Becker <michael@bemit.codes> (https://bemit.codes)",
    "license": "MIT",
    "keywords": [
        "ui-schema",
        "json-schema",
        "mui",
        "material",
        "material-ui",
        "react",
        "reactjs",
        "react-component",
        "react-color",
        "colorpicker",
        "color picker",
        "picker"
    ],
    "homepage": "https://github.com/ui-schema/react-color/tree/main/packages/material-color",
    "repository": {
        "type": "git",
        "url": "https://github.com/ui-schema/react-color.git",
        "directory": "packages/material-color"
    },
    "bugs": {
        "url": "https://github.com/ui-schema/react-color/issues"
    },
    "main": "./index.cjs",
    "module": "./esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./esm/index.js",
            "require": "./index.cjs"
        },
        "./*": {
            "types": "./*/index.d.ts",
            "import": "./esm/*/index.js",
            "require": "./*/index.cjs"
        },
        "./package.json": "./package.json",
        "./esm": null
    },
    "scripts": {
        "dtsgen": "tsc -d --emitDeclarationOnly --pretty && node ../../tools/merge-dirs.js build/dts build && rm -rf build/dts && rm -rf build/**/*.test.* build/**/*.mock.*",
        "__release": "cp package.json build/ && cp package-lock.json build/ && cp README.md build/ && npm publish build"
    },
    "sideEffects": false,
    "dependencies": {
    },
    "devDependencies": {
        "@types/react": "^19.0",
        "@types/react-color": "^3.0.12",
        "@mui/material": "^5.1",
        "@mui/icons-material": "^5.1",
        "@ui-schema/ds-material": "~0.5.0-alpha.1",
        "@ui-schema/ui-schema": "~0.5.0-alpha.1",
        "@ui-schema/react": "~0.5.0-alpha.1",
        "immutable": "^5.0.0",
        "react-color": "^2.0.0",
        "react": "^19.0",
        "react-dom": "^19.0"
    },
    "peerDependencies": {
        "@mui/material": "^5.1 || ^6.1 || ^7.0",
        "@mui/icons-material": "^5.1 || ^6.1 || ^7.0",
        "@ui-schema/ds-material": "~0.5.0-alpha.1",
        "@ui-schema/ui-schema": "~0.5.0-alpha.1",
        "@ui-schema/react": "~0.5.0-alpha.1",
        "immutable": "^4.0.0 || ^5.0.0",
        "react-color": "^2.0.0",
        "@types/react-color": "^3.0.12",
        "react": "^18.0 || ^19.0"
    },
    "peerDependenciesMeta": {
        "@types/react-color": {
            "optional": true
        }
    },
    "publishConfig": {
        "tag": "next",
        "access": "public"
    }
}
